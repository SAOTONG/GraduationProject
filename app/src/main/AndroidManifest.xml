<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sao.graduationproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".main.ui.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".main.ui.LoginActivity"></activity>
        <activity android:name=".main.ui.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.ui.UserActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".main.ui.CompetitionDetailsActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".main.ui.ApplySuccessActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".main.ui.CompetitionApplyActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".main.ui.MyCompetitionActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>

        <activity android:name=".main.ui.MyCompetitionDetailsActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".main.ui.MyProjectActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".main.ui.MyAchievementActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>